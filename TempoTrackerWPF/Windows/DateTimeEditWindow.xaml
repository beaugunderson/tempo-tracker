<Window x:Class="TempoTrackerWPF.Windows.DateTimeEditWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Edit Entry" Height="152" Width="284" xmlns:extToolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit/extended" MinHeight="152" MinWidth="284">
    <Grid>
        <extToolkit:DecimalUpDown Margin="73,12,12,0" Name="entryDecimalUpDown" VerticalAlignment="Top" />
        <DatePicker Height="25" Margin="73,42,12,0" Name="entryDatePicker" VerticalAlignment="Top" />
        <Button Content="_Cancel" HorizontalAlignment="Right" Margin="0,0,93,12" Name="cancelButton" Width="75" IsCancel="True" Height="23" VerticalAlignment="Bottom" Click="cancelButton_Click" />
        <Button Content="_OK" Margin="0,0,12,12" Name="okButton" IsDefault="True" Height="23" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75" Click="okButton_Click" />
        <Label Content="Hours" Height="28" HorizontalAlignment="Left" Margin="12,10,0,0" Name="label1" VerticalAlignment="Top" />
        <Label Content="Date" Height="28" HorizontalAlignment="Left" Margin="12,41,0,0" Name="label2" VerticalAlignment="Top" />
    </Grid>
</Window>
